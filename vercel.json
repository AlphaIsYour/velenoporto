{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.2",
            "config": {
                "php": "8.2"
            }
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|fonts|favicon|assets|build)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "velenoporto",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "${VERCEL_URL}",
        "APP_KEY": "base64:WK1qtS1hTcYj18raH/zpxaghRQC0DGsomDZnjLjLyD4="
    },
    "build": {
        "env": {
            "LD_LIBRARY_PATH": "/usr/lib64:/usr/lib"
        }
    }
}
